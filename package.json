{
  "name": "node-timecodes",
  "version": "2.5.0",
  "description": "Time and SMTPE Timecodes conversions",
  "license": "MIT",
  "repository": "Synchronized-TV/node-timecodes",
  "author": {
    "name": "Julien Bouquillon",
    "email": "julien.bouquillon@synchronized.tv",
    "url": "http://github.com/Synchronized-TV"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "babel-node spec | tap-spec",
    "build": "babel src --out-dir dist",
    "prepare": "npm run test && npm run build"
  },
  "main": "dist/index.js",
  "bin": {
    "timecodeFromSeconds": "dist/fromSeconds-bin.js",
    "timecodeToSeconds": "dist/toSeconds-bin.js"
  },
  "keywords": [
    "node",
    "timecode",
    "smtpe",
    "time",
    "conversion"
  ],
  "testling": {
    "files": "test/*.js",
    "browsers": [
      "ie/6..latest",
      "chrome/22..latest",
      "firefox/16..latest",
      "safari/latest",
      "opera/11.0..latest",
      "iphone/5..latest",
      "ipad/5..latest",
      "android-browser/2..latest"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "8.26.0",
    "tap-spec": "4.0.2",
    "tape": "4.0.0"
  }
}
